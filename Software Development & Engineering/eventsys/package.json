{
  "name": "eventsys",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"set NODE_ENV=development\" \"nodemon prod-server/index.js\" \"vue-cli-service serve\" ",
    "build": "set NODE_ENV=production && babel dev-server --out-dir prod-server && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "concurrently \"set NODE_ENV=development\" \"babel dev-server --out-dir prod-server --watch\" \"nodemon prod-server/index.js\" \"npm run serve\" ",
    "test:e2e": "concurrently \"npm run serve\" \"npx --no-install cypress open\" ",
    "test:unit": "vue-cli-service test:unit",
    "mock:data": "node prod-server/mock/data.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "axios": "^0.19.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.4",
    "core-js": "^3.3.2",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.7",
    "popper.js": "^1.16.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-e2e-cypress": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-unit-mocha": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "chai": "^4.1.2",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
    "vue-template-compiler": "^2.6.10"
  }
}
